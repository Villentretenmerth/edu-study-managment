<div class="content--center">
	<div class="settings">

		<div class="content--center">
			<div class="settings__row" style="width:400px;"> 
				Dodaj nowy moduł:
			</div>
		</div>

		<div class="content--center">
			<div class="settings__row"> 
				Nazwa: 
			</div>
			<div class="settings__row"> 
				<input type="text" ng-model="newModuleText">
			</div>
		</div>

		<div class="content--center">
			<div class="settings__row"> 
				Waga modułu: 
			</div>
			<div class="settings__row"> 
				<select ng-model="newModuleColor">
					<option value="1">Czerwony</option>
					<option value="2">Niebieski</option>
					<option value="3">Zielony</option>
				</select>
			</div>
		</div>

		<div class="content--center">
			<div class="settings__row" style="width:400px;"> 
				<button ng-click="sendNewModule()">Dodaj</button>
			</div>
		</div>

	</div>
</div>